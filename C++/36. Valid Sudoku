https://leetcode.com/problems/valid-sudoku/

class Solution {
public:
    bool isValidSudoku(vector<vector<char>>& board) {
        int R = board.size();
        int C = board[0].size();
        unordered_map<char, int>row[9];
        unordered_map<char, int>col[9];
        unordered_map<char, int>subBoxes[9];
        
        
        for(int i=0; i<R; i++){
            for(int j = 0; j<C ; j++){
                char ch = board[i][j];
                if(ch != '.' and (row[i][ch]++>0 || col[j][ch]++ > 0 || subBoxes[(i/3)*3 + j/3][ch]++ > 0 ))
                    return false;
            }
        }
        return true;
        
    }
};
